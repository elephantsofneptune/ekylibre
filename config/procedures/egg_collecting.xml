<?xml version="1.0"?>
<procedures xmlns="http://www.ekylibre.org/XML/2013/procedures">
  <procedure name="egg_collecting" categories="animal_production" actions="egg_collecting" varieties="aves">
    <parameters>
      <target name="poultry" filter="is aves and can produce(egg)"/>
      <output name="eggs" filter="is egg and derives from aves" default-name="{{variant}} [{{birth_day}}/{{birth_month}}/{{birth_year}}] ({{container}})">
        <attribute name="new_name"/>
        <handler name="population"/>
      </output>
      <doer name="doer" filter="is worker" cardinality="*"/>
    </parameters>
  </procedure>
</procedures>